{
  "name": "fretter",
  "version": "0.0.2",
  "description": "A vue application that helps guitar players with their intervals",
  "author": "Tibor Krizsak",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "lint": "eslint --max-warnings 0 --ext .js,.ts,.vue src",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "bulma": "^0.9.4",
    "eslint": "^8.34.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "pinia": "^2.0.32",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vue-three-shortkey": "^4.0.1"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.58.3",
    "typescript": "~4.7.4",
    "vite": "^4.1.3",
    "vitest": "^0.28.5",
    "vue-tsc": "^1.1.5"
  }
}
